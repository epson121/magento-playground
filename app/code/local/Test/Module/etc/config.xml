<?xml version="1.0" encoding="UTF-8"?>
<config>
    <global>
        <blocks>
            <test_module>
                <class>Test_Module_Block</class>
            </test_module>
            <!--catalog>
                <rewrite>
                    <product_view>Test_Module_Block_Catalog_Product_View</product_view>
                </rewrite>
            </catalog-->
            <page>
                <rewrite>
                    <html_breadcrumbs>Test_Module_Block_Html_Breadcrumbs</html_breadcrumbs>
                </rewrite>
            </page>

        </blocks>
        <helpers>
            <test_module>
                <class>Test_Module_Helper</class>
            </test_module>
        </helpers>
        <models>
            <test_module>
                <class>Test_Module_Model</class>
            </test_module>
        </models>
    </global>

    <frontend>
        <routers>
            <test_module>
                <use>standard</use>
                <args>
                    <module>Test_Module</module>
                    <frontName>test</frontName>
                </args>
            </test_module>
        </routers>
        <layout>
            <updates>
                <my_layout>
                    <file>test_module/custom.xml</file>
                </my_layout>
            </updates>
        </layout>
    </frontend>
</config>